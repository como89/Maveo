<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" stylesheets="StyleDialog.css">
    <left>
        <VBox spacing="15">
            <padding>
                <Insets top = "5" bottom="5" left = "5" right = "5"/>
            </padding>
            <children>
                <Label fx:id="labelTitle" text = "Title" styleClass="label_font"/>
                <Label fx:id="labelArtist" text = "Artist" styleClass="label_font"/>
            </children>
        </VBox>
    </left>
    <right>
        <VBox spacing = "5">
            <padding>
                <Insets top = "5" bottom="5" left = "5" right = "5"/>
            </padding>
            <TextField fx:id="fieldTitle"/>
            <TextField fx:id="fieldArtist"/>
        </VBox>
    </right>
</BorderPane>